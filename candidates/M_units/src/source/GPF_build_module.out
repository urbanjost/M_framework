SYNOPSIS
  elemental function norm_angle_deg(ang) result(rev)

      doubleprecision, intent(in) :: ang
      doubleprecision		  :: rev
	 or
      real, intent(in) :: ang
      real	       :: rev
	 or
      integer, intent(in) :: ang
      integer		  :: rev


DESCRIPTION
